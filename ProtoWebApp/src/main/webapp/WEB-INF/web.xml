<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_5.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>OverviewServlet</servlet-name>
    <servlet-class>com.cliffconsulting.proto.OverviewServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SimBotServlet</servlet-name>
  	<servlet-class>com.cliffconsulting.proto.SimBotServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>OverviewServlet</servlet-name>
  	<url-pattern>/overview</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SimBotServlet</servlet-name>
  	<url-pattern>/simBot</url-pattern>
  </servlet-mapping>

</web-app>
